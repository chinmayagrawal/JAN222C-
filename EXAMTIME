/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception{
	    Scanner sc=new Scanner(System.in);
	    int tc=sc.nextInt();
	    while(tc--!=0){
	        int a=sc.nextInt();
	        int b=sc.nextInt();
	        int c=sc.nextInt();
	        int d=sc.nextInt();
	        int e=sc.nextInt();
	        int f=sc.nextInt();
	        int dt=a+b+c;
	        int st=d+e+f;
	        if(dt>st) System.out.println("Dragon");
	        else if(st>dt) System.out.println("Sloth");
	        else{
	            if(a>d) System.out.println("Dragon");
	            else if(d>a) System.out.println("Sloth");
	            else{
	                if(b>e) System.out.println("Dragon");
	                else if(e>b) System.out.println("Sloth");
	                else System.out.println("Tie");
	            }
	        }
	    }
	}
}
